{
  "name": "storj-bridge-gui",
  "version": "0.0.1",
  "description": "A GUI dashboard for Storj Bridge",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Storj/bridge-gui"
  },
  "license": "AGPL-3.0",
  "homepage": "https://app.storj.io/",
  "main": "bin/server.js",
  "scripts": {
    "mock-backend": "node ./scripts/test/mock-backend/index.js",
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run start-dev",
    "dev": "concurrent --kill-others \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "NODE_ENV=test PORT=4000 node ./bin/test.js",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "dependencies": {
    "apollo-client": "^0.4.19",
    "assets-webpack-plugin": "^3.4.0",
    "async": "^2.0.0-rc.2",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "better-npm-run": "^0.0.4",
    "bootstrap-sass": "3.3.6",
    "bootstrap-sass-loader": "^1.0.9",
    "camelcase": "^3.0.0",
    "clean-webpack-plugin": "^0.1.4",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.0",
    "css-loader": "^0.23.1",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.5",
    "multireducer": "^2.0.0",
    "node-sass": "^3.4.2",
    "node-uuid": "^1.4.7",
    "pretty-error": "^1.2.0",
    "react": "^0.14.7",
    "react-apollo": "^0.3.20",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.1",
    "react-helmet": "^2.2.0",
    "react-loader": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "redux": "^3.0.4",
    "redux-form": "^4.2.2",
    "request": "^2.69.0",
    "sass-loader": "^3.1.2",
    "serve-favicon": "^2.3.0",
    "shebang-loader": "0.0.1",
    "shelljs": "^0.7.5",
    "storj-lib": "^6.0.2",
    "strip-loader": "^0.1.0",
    "stripe": "^4.9.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "apollo-server": "^0.2.1",
